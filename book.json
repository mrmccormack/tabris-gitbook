{
    "plugins": [ "theme-api" ],
    "pluginsConfig": {
        "theme-api": {
            "languages": [
                {
                    "lang": "js",
                    "name": "JavaScript",
                    "default": true
                },
                {
                    "lang": "go",
                    "name": "Go"
                }
            ]
        }
    }

    {
        "plugins": ["github-embed"],
        "pluginsConfig": {
            "github-embed": {
                "showLink": true, 
                "reindent": true 
            }
        }
    }


}

